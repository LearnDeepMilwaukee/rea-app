{
  "name": "@freedomcoop/ocp-app",
  "version": "0.0.0",
  "private": true,
  "main": "packages/app",
  "scripts": {
    "postinstall": "npm run bootstrap && typings install",
    "bootstrap": "lerna bootstrap --npm-client=yarn",
    "test": " cd packages/app && npm test",
