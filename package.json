{
  "name": "@freedomcoop/ocp-app",
  "version": "0.0.0",
  "private": true,
  "main": "src",
  "scripts": {
    "start": "node .",
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src .storybook",
    "dev": "concurrently --kill-others \"nodemon --watch webpack/webpack-assets.json src\" \"node webpack/webpack-dev-server\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build:clean": "rimraf \"dist/**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "modulePathIgnorePatterns": [],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^(components|containers)$": "<rootDir>/test/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/test/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/test/actionsMock.js"
    }
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "concurrently": "^3.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-react": "^6.10.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^19.0.0",
    "loader-utils": "^1.1.0",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^9.1.0",
    "postcss-reporter": "^3.0.0",
    "postcss-scss": "^0.4.1",
    "postcss-strip-inline-comments": "^0.1.5",
    "postcss-url": "^5.1.2",
    "react-addons-test-utils": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "serve-static": "^1.12.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.2.4",
    "eslint-plugin-flowtype": "^2.30.4",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "history": "3.0.0",
    "i18n-react": "^0.3.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "morgan": "^1.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-svg-loader": "^1.1.1",
    "react-themeable": "^1.1.0",
    "react-toolbox": "2.0.0-beta.7",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.3.4",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-saga": "^0.14.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.6.1",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.1",
    "webpack-isomorphic-tools": "^3.0.2",
    "webpack-md5-hash": "^0.0.5",
    "yaml-loader": "^0.4.0"
  }
}
