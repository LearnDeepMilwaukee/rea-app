    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "@types/react": "^16.0.34",
    "@types/tape": "^4.2.31",
    "babel-tape-runner": "^2.0.1",
    "lerna": "^2.0.0-rc.4",
    "redux-devtools-extension": "^2.13.2"
  },
  "dependencies": {
    "cross-env": "^5.1.3"
  }
