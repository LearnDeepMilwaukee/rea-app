{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-flow-strip-types",
    ["react-transform", {
      "transforms": [{
        "transform": "react-transform-catch-errors",
        "imports": [
          "react",
          "redbox-react"
        ]
      }]
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
