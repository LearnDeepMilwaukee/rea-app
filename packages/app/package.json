{
  "name": "@vflows/app",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run remotedev\" \"nodemon -w webpack/webpack-assets.json -w node_modules/@vflows .\" \"node webpack/webpack-dev-server\"",
    "remotedev": "remotedev --hostname=localhost --port=7999",
    "test": "cross-env NODE_ENV=test node -r @vflows/test-helpers node_modules/.bin/ava",
    "coverage": "nyc --reporter=lcov --reporter=html npm run test && nyc report",
    "lint": "eslint .",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config",
    "build:clean": "rimraf \"dist/**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "heroku-postbuild": "npm run build"
  },
  "ava": {
    "files": [
      "*.test.js",
      "**/*.test.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "@vflows/test-helpers": "0.0.0",
    "ava": "^0.19.1",
    "babel-eslint": "^7.2.0",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "concurrently": "^3.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "0.26.4",
    "enzyme": "^2.7.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-react": "^6.10.3",
    "loader-utils": "^1.1.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "postcss-loader": "1.1.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^9.1.0",
    "postcss-reporter": "^3.0.0",
    "postcss-scss": "^0.4.1",
    "postcss-strip-inline-comments": "^0.1.5",
    "postcss-url": "^5.1.2",
    "react-addons-test-utils": "^15.4.1",
    "serve-static": "^1.12.1",
    "sinon": "^2.1.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "@vflows/store": "0.0.0",
    "@vflows/bindings": "0.0.0",
    "@vflows/views": "0.0.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.2.4",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "history": "3.0.0",
    "i18n-react": "^0.3.0",
    "json-loader": "^0.5.4",
    "morgan": "^1.7.0",
    "react": "^15.4.1",
    "react-apollo": "^1.0.0",
    "react-dom": "^15.4.1",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "^3.0.0",
    "react-svg-loader": "^1.1.1",
    "react-transform-catch-errors": "^1.0.2",
    "recompose": "^0.22.0",
    "redbox-react": "^1.3.4",
    "remotedev-server": "^0.3.0-beta-3",
    "rimraf": "^2.6.1",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.1",
    "webpack-isomorphic-tools": "^3.0.2",
    "webpack-md5-hash": "^0.0.5",
    "yaml-loader": "^0.4.0"
  }
}
